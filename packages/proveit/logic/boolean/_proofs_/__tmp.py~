import proveit
context = proveit.Context('..') # the theorem's context is in the parent directory
__locals = locals()
__proving_theorem = proveit.Context('..').getTheorem(trueEqTrue)
__proving_theorem_truth = proving_theorem.provenTruth
__proving_theorem_truth = proving_theorem.provenTruth
